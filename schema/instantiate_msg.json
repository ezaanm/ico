{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "base_conv_ratio",
    "decimals",
    "fundraise_denom",
    "fundraise_goal",
    "name",
    "symbol"
  ],
  "properties": {
    "base_conv_ratio": {
      "description": "conversion ratio of fundraise_denom:derivative_token",
      "allOf": [
        {
          "$ref": "#/definitions/Decimal"
        }
      ]
    },
    "decimals": {
      "description": "decimal places of the derivative token (for UI)",
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "fundraise_denom": {
      "description": "denom of coins sent to this contract for fundraising",
      "type": "string"
    },
    "fundraise_goal": {
      "description": "goal fundraise amount",
      "allOf": [
        {
          "$ref": "#/definitions/Uint128"
        }
      ]
    },
    "name": {
      "description": "name of the derivative token",
      "type": "string"
    },
    "rates": {
      "description": "nullable field of Rates",
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/Rate"
      }
    },
    "symbol": {
      "description": "symbol / ticker of the derivative token",
      "type": "string"
    }
  },
  "definitions": {
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "Rate": {
      "type": "object",
      "required": [
        "min",
        "ratio"
      ],
      "properties": {
        "min": {
          "description": "min fundraise_denom sent for this rate",
          "allOf": [
            {
              "$ref": "#/definitions/Uint128"
            }
          ]
        },
        "ratio": {
          "description": "conversion rate from fundraise_denom:ASSET",
          "allOf": [
            {
              "$ref": "#/definitions/Decimal"
            }
          ]
        }
      }
    },
    "Uint128": {
      "type": "string"
    }
  }
}
